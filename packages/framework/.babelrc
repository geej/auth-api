  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    [ "@babel/plugin-proposal-decorators", { "legacy": true } ],
    [ "@babel/plugin-transform-react-jsx", { "pragma": "h" } ]
  ],
  "presets": [
    ["@babel/preset-env", {
      "targets": {
